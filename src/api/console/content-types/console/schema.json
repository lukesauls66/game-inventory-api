{
  "kind": "collectionType",
  "collectionName": "consoles",
  "info": {
    "singularName": "console",
    "pluralName": "consoles",
    "displayName": "Console"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 150,
      "required": true,
      "unique": true
    },
    "manufacturer": {
      "type": "string"
    },
    "releaseYear": {
      "type": "integer"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "games": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::game.game",
      "mappedBy": "consoles"
    },
    "inventory_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory-item.inventory-item",
      "mappedBy": "console"
    }
  }
}
